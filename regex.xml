<?xml version="1.0" encoding="iso-8859-1"?>
<reglas>
	<revista issn="1026-8774">
		<nombre>Revista mexicana de ciencias geológicas</nombre>
		<regex>(.*?)[,.]\ ((?:19|20)[0-9]{2}[a-z]*)[,.]\ (.*):\ (.*)\.</regex>
	</revista>
	<revista issn="0185-3880">
		<nombre>Ciencias marinas</nombre>
		<regex>(.*?)\.\ ((?:19|20)[0-9]{2}[a-z]*)[,.]\ (.*?)(?:\.\ In:\ |\.\ )(.*)</regex>
	</revista>
	<revista issn="0187-6236">
		<nombre>Atmósfera</nombre>
		<regex>(.*?,\ )((?:19|20)[0-9]{2}[a-z]*)([,.]\ )(.*?)(\.\ In:\ |\.\ )(.*)</regex>
		<grupos>
			<contrib>
				<autores notag="true">
					<value>$1</value>
					<regex>(.*?)(,\ |\ and\ )</regex>
					<grupos>
						<autor>
							<value>$1</value>
							<regex>(.*?)(\ )([A-Z]\.\ [A-Z]\.|[A-Z]\.)</regex>
							<grupos>
								<fname>
									<value>$1</value>
									<separator>$2</separator>
								</fname>
								<lname>
									<value>$3</value>
								</lname>
							</grupos>
							<separator>$2</separator>
						</autor>
					</grupos>
				</autores>
				<fecha notag="true">
					<value>$2</value>
					<regex>((?:19|20)[0-9]{2})([a-z]*)</regex>
					<grupos>
						<fecha>
							<value>$1</value>
							<separator>$2</separator>
						</fecha>
					</grupos>
					<separator>$3</separator>
				</fecha>
				<titulo>
					<value>$4</value>
					<separator>$5</separator>
				</titulo>
			</contrib>
			<revista>
				<value>$6</value>
			</revista>
		</grupos>
	</revista>
	<revista issn="0185-3325">
		<nombre>Salud Mental</nombre>
		<regex>([0-9]+)\.\ (.+?)\.\ (.+?)(?:\.\ |\?\ )(.*)</regex>
	</revista>
	<revista issn="1405-7743">
		<nombre>Ingeniería, investigación y tecnología</nombre>
		<regex>((?:.+?\ y\ .+?)|.+?)\.\ (.+?)(?:\.\ |\?\ )(.*)</regex>
	</revista>
</reglas>
