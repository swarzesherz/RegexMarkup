<?xml version="1.0" encoding="utf-8" ?>
<reglas>
  <revista issn="1026-8774">
    <nombre>Revista mexicana de ciencias geológicas</nombre>
    <regex>(.*?)[,.]\ ((?:19|20)[0-9]{2}[a-z]*)[,.]\ (.*):\ (.*)\.</regex>
  </revista>
  <revista issn="0185-3880">
    <nombre>Ciencias marinas</nombre>
    <regex>(.*?)\.\ ((?:19|20)[0-9]{2}[a-z]*)[,.]\ (.*?)(?:\.\ In:\ |\.\ )(.*)</regex>
  </revista>
  <revista issn="0187-6236">
    <nombre>Atmósfera</nombre>
    <regex>(.*?,\ )((?:19|20)[0-9]{2})([a-z]*[,.]\ )(.*?)(\.\ In:\ |\.\ )(.*)</regex>
    <grupos>
      <contrib>
        <autores notag="true">
          <value>$1</value>
          <regex>(.*?)(,\ |\ and\ )</regex>
          <grupos>
            <autor>
              <value>$1</value>
              <multiple>
                <opcion>
                  <regex>([A-Z]\.\ [A-Z]\.|[A-Z]\.)(\ )(.*)</regex>
                  <grupos>
                    <lname>
                      <value>$1</value>
                      <postvalue>$2</postvalue>
                    </lname>
                    <fname>
                      <value>$3</value>
                    </fname>
                  </grupos>
                </opcion>
                <opcion>
                  <regex>(.*?)(\ )([A-Z]\.\ [A-Z]\.|[A-Z]\.)</regex>
                  <grupos>
                    <fname>
                      <value>$1</value>
                      <postvalue>$2</postvalue>
                    </fname>
                    <lname>
                      <value>$3</value>
                    </lname>
                  </grupos>
                </opcion>
              </multiple>
              <postvalue>$2</postvalue>
            </autor>
          </grupos>
        </autores>
        <fecha>
          <value>$2</value>
          <postvalue>$3</postvalue>
        </fecha>
        <titulo>
          <value>$4</value>
        </titulo>
      </contrib>
      <contribin notag="true">
        <value>$5$6</value>
        <multiple>
          <opcion>
            <regex>(\.\ In:\ )(.*)</regex>
            <grupos>
              <monog>
                <prevalue>$1</prevalue>
                <value>$2</value>
              </monog>
            </grupos>
          </opcion>
          <opcion>
            <regex>(\.\ )(.*)</regex>
            <grupos>
              <serial>
                <prevalue>$1</prevalue>
                <value>$2</value>
              </serial>
            </grupos>
          </opcion>
        </multiple>
      </contribin>
    </grupos>
  </revista>
  <revista issn="0185-3325">
    <nombre>Salud Mental</nombre>
    <regex>([0-9]+)\.\ (.+?)\.\ (.+?)(?:\.\ |\?\ )(.*)</regex>
  </revista>
  <revista issn="1405-7743">
    <nombre>Ingeniería, investigación y tecnología</nombre>
    <regex>((?:.+?\ y\ .+?)|.+?)\.\ (.+?)(?:\.\ |\?\ )(.*)</regex>
  </revista>
</reglas>
